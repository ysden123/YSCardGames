<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.1.3</AssemblyVersion>
    <ApplicationIcon>YSCardGamesApp.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="YSCardGamesApp.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="YSCardGamesApp.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\YSCardGamesLibrary\YSCardGamesLibrary.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="common">
      <HintPath>..\..\common-cs\common\bin\Release\net10.0-windows\common.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
